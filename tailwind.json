{
	"pointer-events-auto": {
		"style": {
			"pointerEvents": "auto"
		}
	},
	"absolute": {
		"style": {
			"position": "absolute"
		}
	},
	"mt-3": {
		"style": {
			"marginTop": 12
		}
	},
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"h-full": {
		"style": {
			"height": "100%"
		}
	},
	"w-full": {
		"style": {
			"width": "100%"
		}
	},
	"flex-col": {
		"style": {
			"flexDirection": "column"
		}
	},
	"rounded-md": {
		"style": {
			"borderTopLeftRadius": 6,
			"borderTopRightRadius": 6,
			"borderBottomRightRadius": 6,
			"borderBottomLeftRadius": 6
		}
	},
	"bg-blue-400": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(96 165 250 / var(--tw-bg-opacity))"
		}
	},
	"p-3": {
		"style": {
			"paddingTop": 12,
			"paddingRight": 12,
			"paddingBottom": 12,
			"paddingLeft": 12
		}
	},
	"p-4": {
		"style": {
			"paddingTop": 16,
			"paddingRight": 16,
			"paddingBottom": 16,
			"paddingLeft": 16
		}
	},
	"px-5": {
		"style": {
			"paddingLeft": 20,
			"paddingRight": 20
		}
	},
	"py-3": {
		"style": {
			"paddingTop": 12,
			"paddingBottom": 12
		}
	},
	"pt-10": {
		"style": {
			"paddingTop": 40
		}
	},
	"text-lg": {
		"style": {
			"fontSize": 18,
			"lineHeight": 28
		}
	},
	"text-xl": {
		"style": {
			"fontSize": 20,
			"lineHeight": 28
		}
	},
	"font-bold": {
		"style": {
			"fontWeight": "700"
		}
	},
	"text-white": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(255 255 255 / var(--tw-text-opacity))"
		}
	},
	"hover:bg-blue-300:hover": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(147 197 253 / var(--tw-bg-opacity))"
		}
	}
}